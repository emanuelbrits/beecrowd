var input = require('fs').readFileSync('/dev/stdin', 'utf8');
var lines = input.split('\n');

function main(){

    let numero = Number(lines[0])
    let contador = 0
    let num = 1

    while(contador < numero){
        contador++
        
        console.log(`${num++} ${num++} ${num++} PUM`)
        num++
    }

}
main()
